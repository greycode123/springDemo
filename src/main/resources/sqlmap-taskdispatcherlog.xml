<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap
        PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="userQueue">

    <insert id="INSERT-TASKDISPATCHERLOG" parameterClass="com.smart.dispatcher.domain.TaskDispatcherLog">
        INSERT INTO  task_dispatcher_log (
        id ,
        task_id,
        task_status ,
        user_name,
        dispatcher_time
        )
        VALUES (
        #ID#,
        #taskID#,
        #taskStatus#,
        #userName#,
        #dispatherTime#
        )
    </insert>

</sqlMap>